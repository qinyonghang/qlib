@PACKAGE_INIT@

set(LIB_NAME @LIB_NAME@)

add_library(${LIB_NAME}::${LIB_NAME} STATIC IMPORTED)
if (CMAKE_SYSTEM_NAME STREQUAL "Windows")
set_target_properties(${LIB_NAME}::${LIB_NAME} PROPERTIES
    IMPORTED_LOCATION_DEBUG ${PACKAGE_PREFIX_DIR}/lib/${LIB_NAME}.lib
    IMPORTED_LOCATION_RELEASE ${PACKAGE_PREFIX_DIR}/lib/${LIB_NAME}.lib
    IMPORTED_LOCATION_MINSIZEREL ${PACKAGE_PREFIX_DIR}/lib/${LIB_NAME}.lib
    IMPORTED_LOCATION_RELWITHDEBINFO ${PACKAGE_PREFIX_DIR}/lib/${LIB_NAME}.lib
    INTERFACE_INCLUDE_DIRECTORIES ${PACKAGE_PREFIX_DIR}/include
)
else ()
set_target_properties(${LIB_NAME}::${LIB_NAME} PROPERTIES
    IMPORTED_LOCATION ${PACKAGE_PREFIX_DIR}/lib/lib${LIB_NAME}.a
    INTERFACE_INCLUDE_DIRECTORIES ${PACKAGE_PREFIX_DIR}/include
)
endif ()

set(${LIB_NAME}_INCLUDE_DIRS ${PACKAGE_PREFIX_DIR}/include)
set(${LIB_NAME}_LIBRARY_DIRS ${PACKAGE_PREFIX_DIR}/lib)
set(${LIB_NAME}_LIBRARIES ${LIB_NAME}::${LIB_NAME})

string(TOUPPER ${LIB_NAME} LIB_NAME_UPPER)
set(${LIB_NAME_UPPER}_INCLUDE_DIRS ${PACKAGE_PREFIX_DIR}/include)
set(${LIB_NAME_UPPER}_LIBRARY_DIRS ${PACKAGE_PREFIX_DIR}/lib)
set(${LIB_NAME_UPPER}_LIBRARIES ${LIB_NAME}::${LIB_NAME})

check_required_components(${LIB_NAME})
